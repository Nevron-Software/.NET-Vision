<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NStandardLine3DUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
		<Label Content="Line Style:" Height="28" Name="label3" />
		<ComboBox Height="23" Name="LineStyleComboBox" SelectionChanged="LineStyleComboBox_SelectionChanged" />
		<CheckBox Content="Inflate Margins" Height="16" Name="InflateMarginsCheckBox" Click="InflateMarginsCheckBox_Checked" />
		<CheckBox Content="Axis Round To Tick" Height="16" Name="AxisRoundToTickCheckBox" Click="AxisRoundToTickCheckBox_Checked" />
	</StackPanel>
</Controls:NExampleBaseUC>
