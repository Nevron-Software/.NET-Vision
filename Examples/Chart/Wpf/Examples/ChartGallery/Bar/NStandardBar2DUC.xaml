<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NStandardBar2DUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
		<Label Content="Bar Style:" Height="28" Name="label3" />
		<ComboBox Height="23" Name="BarStyleComboBox" SelectionChanged="BarStyleComboBox_SelectionChanged" />
		<Label Content="Edge Percent:" Height="28" Name="label4" />
		<ComboBox Height="23" Name="BarEdgePercentComboBox" SelectionChanged="BarEdgePercentComboBox_SelectionChanged" />
		<CheckBox Content="Has Top Edge" Height="16" Name="HasTopEdgeCheckBox" Click="HasTopEdgeCheckBox_Checked" />
		<CheckBox Content="Has Bottom Edge" Height="16" Name="HasBottomEdgeCheckBox" Click="HasBottomEdgeCheckBox_Checked" />
		<CheckBox Content="Different Fill Styles" Height="16" Name="DifferentFIllStylesCheckBox" Click="DifferentFillStylesCheckBox_Checked" />
	</StackPanel>
</Controls:NExampleBaseUC>
