<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NErrorBarUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
		<CheckBox Content="Show Upper Error" Height="16" Name="ShowUpperErrorCheckBox" Click="ShowUpperErrorCheckBox_Checked" />
		<CheckBox Content="Show Lower Error" Height="16" Name="ShowLowerErrorCheckBox" Click="ShowLowerErrorCheckBox_Checked" />
		<CheckBox Content="Enable 3D" Height="16" Name="Enable3DCheckBox" Click="Enable3DCheckBox_Checked" />
	</StackPanel>
</Controls:NExampleBaseUC>
