<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NStockStickUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
		<CheckBox Content="Show Open" Height="16" Name="ShowOpenCheckBox" Click="ShowOpenCheckBox_Checked" />
		<CheckBox Content="Show Close" Height="16" Name="ShowCloseCheckBox" Click="ShowCloseCheckBox_Checked" />
		<CheckBox Content="Show High Low" Height="16" Name="ShowHighLowCheckBox" Click="ShowHighLowCheckBox_Checked" />
	</StackPanel>
</Controls:NExampleBaseUC>
