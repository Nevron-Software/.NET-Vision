<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NHeatMapContourUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<StackPanel>
        <ComboBox Name="ContourDisplayModeCombo" SelectionChanged="ContourDisplayModeCombo_SelectionChanged" />
        <ComboBox Name="ContourColorModeCombo" SelectionChanged="ContourColorModeCombo_SelectionChanged"/>
        <CheckBox Name="ShowFillCheckBox" Content="Show Fill" Click="ShowFillCheckBox_Click"/>
        <CheckBox Name="SmoothPaletteCheckBox" Content="Smooth Palette" Click="SmoothPaletteCheckBox_Click"/>
	</StackPanel>
</Controls:NExampleBaseUC>
