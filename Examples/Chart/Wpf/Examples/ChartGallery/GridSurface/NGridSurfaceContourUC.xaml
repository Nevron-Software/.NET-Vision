<Controls:NExampleBaseUC x:Class="Nevron.Examples.Chart.Wpf.NGridSurfaceContourUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:Controls="clr-namespace:Nevron.Examples.Chart.Wpf" 		 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
		<CheckBox Content="Show Filling" Margin ="4" Height="16" Name="ShowFillingCheckBox" Click="ShowFillingCheckBox_Click"   />
		<CheckBox Content="Show Frame" Margin ="4" Height="16" Name="ShowFrameCheckBox" Click="ShowFrameCheckBox_Click" />
		<CheckBox Content="Palette Frame" Margin ="4" Height="16" Name="PaletteFrameCheckBox" Click="PaletteFrameCheckBox_Click" />
		<CheckBox Content="Smooth Palette" Margin ="4" Height="16" Name="SmoothPaletteCheckBox" Click="SmoothPaletteCheckBox_Click" />
		<CheckBox Content="Draw Contour Border" Margin ="4" Height="16" Name="DrawContourBorderCheckBox" Click="DrawContourBorderCheckBox_Click" />
	</StackPanel>
</Controls:NExampleBaseUC>
